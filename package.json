{
  "name": "nestlink",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "npm run build:web && npm run build:func",
    "build:web": "vue-tsc --noEmit && vite build",
    "build:func": "rollup -c",
    "start:func": "tigodevs start -t ./.tigodev.json -r ./rollup.config.js",
    "dev:func": "npm run start:func",
    "deploy:func": "tigoldep -t ./.tigodev.json",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "clipboard-polyfill": "^3.0.3",
    "less": "^4.1.1",
    "lottie-web": "^5.7.13",
    "lru-cache": "^6.0.0",
    "nanoid": "^3.1.28",
    "vue": "^3.2.19"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@backrunner/rollup-plugin-string": "^3.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@tigojs/dev-server": "^0.4.0",
    "@tigojs/lambda-deploy-script": "^0.1.1",
    "@tigojs/lambda-router": "^0.3.0",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^16.10.1",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.19",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-ali": "^12.2.2",
    "eslint-plugin-import": "^2.24.2",
    "f2elint": "^1.2.1",
    "rollup": "^2.57.0",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vue-tsc": "^0.2.3"
  }
}
